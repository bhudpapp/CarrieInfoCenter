{
  "type": "object",
  "properties": {
    "trackingNumber": {
      "type": "string",
      "selector": "input.input-medium.search-query",
      "control": "text"
    },
    "isLanding": {
      "type": "identifier",
      "selector": ":not(#SearchResult)",
      "control": "exists"
    },
    "isNotLanding": {
      "type": "identifier",
      "selector": "#SearchResult",
      "control": "exists"
    },
    "search": {
      "type": "action",
      "selector": ".btn.preventMultiClick",
      "control": "action.button"
    }
  },
  "connect": [
    {
      "match": [
        "isLanding",
        "isNotLanding"
      ],
      "screens": [
        "landing"
      ]
    }
  ]
}
